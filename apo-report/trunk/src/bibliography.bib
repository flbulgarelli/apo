@techreport{mislevy2008dpa,
  abstract = {Assessment design patterns lay out considerations for building assessment tasks that address targeted aspects or situations of language use. They differ from test specifications by focusing on assessment arguments at a narrative level and are organized around aspects of language use that could be detailed in different ways for different purposes or circumstances. A design pattern thus encapsulates experience and research about some aspect of language use, organized around the structure of assessment arguments, as a starting point for designing tasks that will in addition satisfy the constraints and purposes of the job at hand. The rationale and structure of design patterns are described, and their use in language assessment is illustrated with examples concerning the use of language for special purposes, contextualized listening skills, and content-specific story-tellings.},
  added-at = {2008-08-15T20:01:09.000+0200},
  author = {Wei, Hua and Mislevy, Robert J. and Kanal, Diwakar},
  biburl = {http://www.bibsonomy.org/bibtex/2fd1b4c45127e0eeb0a8fbc36de950869/yish},
